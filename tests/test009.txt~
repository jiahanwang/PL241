// Nested if statement test
// Added final let statement to test that phi propagation continues after all
// the phi instructions.
main
var a, b, c;
{
	if a > b then
		if a < b then
			if c != a then
				let c <- a
			fi
		fi
	else
		if a < b then
			let a <- a - 2
		else
			let a <- a + 3
		fi
	fi;

	call OutputNum(a)
}
.
